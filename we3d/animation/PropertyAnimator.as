package we3d.animation {	import we3d.animation.IChannel;		/** 	* PropertyAnimator override a numeric property with the values from an EnvelopeChannel. 	* PropertyAnimators can be created with the Animated transform (core.transform.Animated) and its addChannel method.	*/	public class PropertyAnimator 	{		public function PropertyAnimator (t:Object, p:String, e:IChannel) {			target = t;			prop = p;			env = e;		}				/**		* The parent object of the property to animate 		*/		public var target:Object;		/** 		* The name of the property to animate (the animated property is <code>target[prop]</code>)		*/		public var prop:String;		/**		 * Reference to a channel		 */		public var env:IChannel;	}}